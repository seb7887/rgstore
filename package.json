{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Sebastian Matias Segura (@seb7887)",
  "dependencies": {
    "dotenv": "^6.2.0",
    "gatsby": "^2.1.19",
    "gatsby-image": "^2.0.30",
    "gatsby-plugin-manifest": "^2.0.20",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.7",
    "gatsby-plugin-sharp": "^2.0.23",
    "gatsby-remark-images": "^3.0.6",
    "gatsby-source-contentful": "^2.0.35",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-remark": "^2.3.0",
    "gatsby-transformer-sharp": "^2.1.15",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "babel-jest": "^24.1.0",
    "babel-preset-gatsby": "^0.1.8",
    "cypress": "^3.1.5",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^1.1.1",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.2",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.0",
    "start-server-and-test": "^1.7.11"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e:dev": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:run": "start-server-and-test dev http://localhost:8000 cy:run",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
